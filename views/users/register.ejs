<!DOCTYPE html>
<html lang="en">
<!-- HEAD -->
<%- include('../partials/head.ejs') %> 
<body>
     
<!-- HEADER -->
<%- include('../partials/header.ejs') %> 
        

 <main class="register">
                     
    <form action="/" method="POST">
        <div class="nombre-usuario-email">
            <div>
                <h2 class="nuevo-cliente">NUEVO CLIENTE</h2> 
            </div>
            <div>
                <h4>Nombre</h4>
                <input type="text" name="nombre" placeholder="Ingresa Nombre">
            </div> 
            <div>
                <h4>Apellido</h4>
                <input type="text" name="apellido" placeholder="Ingresa Apellido">
            </div>
            <div>
                <h4>Documento</h4>
                <input type="text" name="documento" placeholder=" Ingresa Documento">
            </div>
            <div>
                <h4>Email</h4>
                <input type="email" name="email" placeholder="Ingresa E-mail"> 
            </div>
            <div>
                <h4>Contraseña</h4>
                <input type="password" name="pass" placeholder="Elegí tu contraseña">
            </div>
            <div>
                <h4>Confirma Contraseña</h4>
                <input type="password" name="pass_confirm" placeholder="Confirmá tu clave">
            </div>
            <div>
                <h4>Domicilio</h4>
                <input type="text" name="addres" placeholder="Ingresa tu Domicilio">
            </div>                   
            <div>
                <h4>Ciudad</h4>
                <input type="text" name="ciudad" placeholder="Ingresa tu Ciudad">   
            </div>
                <a class="registro-R" href="#">REGISTRARSE</a>
        </div>               
        

        
    </form>
</main>

        <%- include('../partials/footer')  %> 
    </body>
</html>